  "bugs": {
    "url": "https://github.com/kevinmccartney/we-the-party-server/issues"
  },
  "homepage": "https://github.com/kevinmccartney/we-the-party-server#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "eslint-plugin-react": "^7.8.2",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "pug": "^2.0.3",
