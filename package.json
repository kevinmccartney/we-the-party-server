{
  "name": "wtp-server",
  "version": "0.0.2",
  "description": "This is the server for the hosting the static We the Party web app as well as the web services consumed by the WTP web app.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src",
    "pre-push": "lint",
    "start:server": "nodemon src/index.js --exec babel-node --presets es2015,stage-2",
    "start:webpack": "webpack --watch --config webpack.config.js",
    "build:client": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinmccartney/we-the-party-server.git"
  },
